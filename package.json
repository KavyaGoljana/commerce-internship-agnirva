{
  "name": "e-commerce",
  "version": "1.0.0",
  "description": "E-commerce website with testing",
  "main": "script.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "start": "node server.js",
    "dev": "nodemon server.js"
  },
  "keywords": [
    "ecommerce",
    "shopping",
    "electronics"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "jest": "^29.7.0"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "verbose": true,
    "collectCoverageFrom": [
      "*.js",
      "!*.test.js",
      "!node_modules/**"
    ],
    "coverageReporters": [
      "text",
      "html"
    ]
  }
}
